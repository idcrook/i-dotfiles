{
  "segments": [
    "virtual_env",
    "ssh",
    "username",
    "hostname",
    "cwd",
    "git",
    "git_stash",
    "set_term_title",
    "read_only",
    "exit_code",
    "newline",
    "root"
  ],
  "mode": "patched",
  "cwd":
  {
    "full_cwd": "true",
    "max_depth": 99,
    "mode": "plain"
  },
  "hostname":
  {
    "colorize": true
  },
  "vcs":
  {
    "show_symbol": true
  },
  "theme": "default"
}
