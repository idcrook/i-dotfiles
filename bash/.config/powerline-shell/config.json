{
  "segments": [
    "virtual_env",
    "ssh",
    "username",
    "hostname",
    "cwd",
    "git",
  {
    "type": "env",
    "var": "OLDPWD"
  },
    "set_term_title",
    "read_only",
    "exit_code",
    "jobs",
    "newline",
    "root"
  ],
  "mode": "patched",
  "cwd":
  {
    "mode": "plain",
    "full_cwd": true
  },
  "hostname":
  {
    "colorize": true
  },
  "vcs":
  {
    "show_symbol": true
  },
  "theme": "default"
}
